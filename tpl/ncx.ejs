<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ncx PUBLIC "-//NISO//DTD ncx 2005-1//EN"
	"http://www.daisy.org/z3986/2005/ncx-2005-1.dtd">

<ncx xmlns="http://www.daisy.org/z3986/2005/ncx/" version="2005-1" xml:lang="en-US">
<head>
<meta name="dtb:uid" content="<%=bookId%>"/>
<meta name="dtb:depth" content="1"/>
<meta name="dtb:totalPageCount" content="0"/>
<meta name="dtb:maxPageNumber" content="0"/>
</head>
<docTitle><text><%=title%></text></docTitle>
<docAuthor><text><%=author%></text></docAuthor>
  <navMap>
  <% for(var year in fileInfo) {%>
  <% var count = 1;%>
  <% for(var month in fileInfo[year]) {%>
    <navPoint class="<%=year%>年<%=month%>" id="<%=year%>年<%=month%>" playOrder="<%=count%>">
      <% count++; %>
      <navLabel>
        <text><%=month%></text>
      </navLabel>
      <content src="<%=fileInfo[year][month][0]%>.html"/>
      <% for(var i=0; i < fileInfo[year][month].length; i++) {%>
        <% var name = fileInfo[year][month][i].substr(11) %>
        <navPoint class="<%=name%>" id="<%=name%>" playOrder="<%=count%>">
        <% count++; %>
          <navLabel>
            <text><%=name%></text>
          </navLabel>
          <content src="<%=fileInfo[year][month][i]%>.html"/>
        </navPoint>
      <% } %>
    </navPoint>
  <% } %>
  <% } %>
  </navMap>
</ncx>